koden til at hente alle person i database
select * from bruger


Foreign Key

select * from konto

inner join bruger

on person_id = id

order by id;


(En start på overførsel)
Update konto set saldo = saldo + 500 where konto_nr = 582102093;
Update konto set saldo = saldo - 500 where konto_nr = 697564654;